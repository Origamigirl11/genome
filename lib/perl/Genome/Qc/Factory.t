#!/usr/bin/env genome-perl

BEGIN { 
    $ENV{UR_DBI_NO_COMMIT} = 1;
    $ENV{UR_USE_DUMMY_AUTOGENERATED_IDS} = 1;
}

use strict;
use warnings;

use above "Genome";
use Test::More;

my $pkg = 'Genome::Qc::Factory';
use_ok($pkg);

my $config = Genome::Qc::Factory::get_config(name => 'test');
ok($config->isa('Genome::Qc::Config'), "Factory gave us a config");
is($config->name, "test", "Config name is correct");

done_testing;

